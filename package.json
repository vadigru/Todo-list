    "gbuild": "gulp build",
    "gstart": "gulp start",
    "wtest": "npm run eslint",
    "wbuild": "webpack --mode production",
    "wstart": "webpack-dev-server"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "browser-sync": "2.26.12",
    "editorconfig": "0.15.3",
    "eslint": "7.7.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.20.6",
    "gulp": "4.0.2",
    "gulp-csso": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "2.6.5",
    "react-emotion": "10.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "markup/*.html",
    "markup/**/*.js",
    "markup/img/**/*.svg",
    "markup/css/**/*.css"
  ],
  "engines": {
    "node": "12.x"
