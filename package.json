{
  "name": "todo_list",
  "version": "1.0.0",
  "private": true,
  "description": "TODO list training task",
  "author": "Vadim Grunenko",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"./markup/**/*.css\" --syntax css",
